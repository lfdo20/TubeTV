{
  "name": "tubetv",
  "version": "1.0.0",
  "description": "Youtube player app simulating classic TV.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "babel": "babel ./src/server.js -o dist/server.js && babel ./src/auth.js -o dist/auth.js",
    "postinstall": "webpack --config ./webpack.config.js --mode production --optimize-minimize && npm run babel",
    "devinstall": "webpack --config ./webpack.config.js --mode development && npm run babel",
    "server": "npm run devinstall && node ./dist/server.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lfdo20/TubeTV.git"
  },
  "keywords": [
    "Youtube",
    "player",
    "react"
  ],
  "author": "Leandro Fernandes lfdo <lfdo20@gmail.com> (https://lfdo20.github.io/lfdo)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lfdo20/TubeTV/issues"
  },
  "homepage": "https://github.com/lfdo20/TubeTV#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "googleapis": "^28.1.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "styled-components": "^3.2.5",
    "yt-player": "^2.5.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.5.1",
    "dotenv": "^5.0.1",
    "jest": "^22.4.3",
    "react-hot-loader": "^4.0.1",
    "regenerator-runtime": "^0.11.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  }
}
